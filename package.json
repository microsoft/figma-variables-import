{
	"name": "figma-variables-import",
	"version": "0.1.0",
	"description": "Figma beta variables import",
	"homepage": "https://www.figma.com/community/plugin/1170488269625782824",
	"author": "Travis Spomer <travis@microsoft.com>",
	"private": true,
	"repository": {
		"type": "private",
		"url": "https://github.com/microsoft/figma-variables-import"
	},
	"scripts": {
		"analyze": "npx webpack --mode production --profile --json > profile.json && npx webpack-bundle-analyzer profile.json build",
		"build": "npx webpack --mode production",
		"build:watch": "npx webpack --mode development --watch",
		"lint": "npx eslint",
		"prettier:format": "npx prettier --write 'src/**/*.{js,jsx,ts,tsx,css,json}'"
	},
	"dependencies": {
		"@travisspomer/promising-artist": "^1.0.0",
		"preact": "^10.11.0",
		"react": "npm:@preact/compat",
		"react-dom": "npm:@preact/compat",
		"styled-components": "^5.3.5"
	},
	"devDependencies": {
		"@figma/plugin-typings": "git+ssh://git@github.com/figma/plugin-typings.git#65781fe71de963668e762634d839dc1a7eea3ce6",
		"@svgr/webpack": "^7.0.0",
		"@types/react": "^18.0.15",
		"@types/react-dom": "^18.0.6",
		"@types/styled-components": "^5.1.25",
		"@typescript-eslint/eslint-plugin": "^5.31.0",
		"@typescript-eslint/parser": "^5.31.0",
		"css-loader": "^6.7.1",
		"eslint": "^8.20.0",
		"html-webpack-plugin": "^5.5.0",
		"node-sass": "^8.0.0",
		"prettier": "^2.7.1",
		"sass-loader": "^13.2.0",
		"style-loader": "^3.3.1",
		"ts-loader": "^9.3.1",
		"tslib": "^2.4.0",
		"typescript": "^4.7.4",
		"webpack": "^5.74.0",
		"webpack-cli": "^4.10.0"
	}
}
